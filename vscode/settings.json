      "display": "none"
    },
    ".sidebar .title-label": {
      "padding": "0 !important"
    },
    ".sidebar": {
      "border": "none !important"
    },
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": {
      "outline": "none !important"
    },
    ".monaco-list-row.focused": {
      "outline": "none !important"
    },
    ".monaco-editor .scroll-decoration": {
      "display": "none"
    },
    ".title-actions": {
      "display": "none !important"
    },
