    ".sidebar > .composite.title::before": "visibility: visible; content: 'Project'; display: flex; justify-content: center; align-items: center; position: absolute; right: 0; left: 0; bottom: 0; top: 0; font-weight: bold !important; font-size: 12px !important; text-transform: uppercase;",
